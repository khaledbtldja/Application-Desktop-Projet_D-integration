<Window x:Class="GestionCanabis.PDFViewerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GestionCanabis"
        mc:Ignorable="d"
        Title="Code QR" Height="250" Width="250"
        Icon="Images/logo.png"
        Closed="Window_Closed">
    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
        <Image x:Name="imagebox" Source="{Binding image}" Width="200" Height="200" HorizontalAlignment="Center" VerticalAlignment="Center"></Image>
    </Grid>
</Window>
